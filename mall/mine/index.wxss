page {
  background-color: #edece8;
}
.user_info_wrap {
  height: 45vh;
  background-color: var(--themeColor);
  overflow: hidden;
}
.user_info_wrap .user_img_wrap {
  position: relative;
}
.user_info_wrap .user_img_wrap .user_bg {
  height: 50vh;
  filter: blur(20rpx);
}
.user_info_wrap .user_img_wrap .user_info {
  position: absolute;
  left: 50%;
  top: 10%;
  transform: translateX(-50%);
  text-align: center;
}
.user_info_wrap .user_img_wrap .user_info .user_icon {
  height: 150rpx;
  width: 150rpx;
  border-radius: 50%;
  margin-bottom: 50rpx;
  overflow: hidden;
}
.user_info_wrap .user_img_wrap .user_info .user_name {
  font-size: 40rpx;
  color: white;
}
.user_content {
  position: relative;
}
.user_content .user_main {
  position: absolute;
  width: 90%;
  left: 50%;
  transform: translateX(-50%);
  top: -40rpx;
  color: #666666;
}
.user_content .user_main .history_wrap {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-around;
  background-color: #fff;
}
.user_content .user_main .history_wrap .single_navi {
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
  padding: 10rpx 0;
}
.user_content .user_main .history_wrap .single_navi .his_num {
  color: red;
}
.user_content .user_main .order_wrap {
  background-color: white;
  margin-top: 30rpx;
}
.user_content .user_main .order_wrap .order_title {
  border-bottom: 1rpx solid #cccccc;
  padding: 20rpx;
}
.user_content .user_main .order_wrap .order_content {
  display: flex;
}
.user_content .user_main .order_wrap .order_content navigator {
  padding: 15rpx 0;
  flex: 1;
  text-align: center;
}
.user_content .user_main .order_wrap .order_content navigator .iconfont {
  color: var(--themeColor);
  font-size: 40rpx;
  margin-bottom: 10rpx;
}
.user_content .user_main .address_wrap {
  margin-top: 30rpx;
  background-color: white;
  padding: 20rpx;
  border-bottom: 1rpx solid #cccccc;
}
.user_content .user_main .app_info_wrap {
  margin-top: 30rpx;
  background-color: #fff;
}
.user_content .user_main .app_info_wrap .app_info_item {
  padding: 20rpx;
  border-bottom: 1rpx solid #cccccc;
}
.user_content .user_main .app_info_contact {
  display: flex;
  justify-content: space-between;
}
.user_content .user_main .command {
  margin: 30rpx 0;
  background-color: white;
  padding: 20rpx;
}
